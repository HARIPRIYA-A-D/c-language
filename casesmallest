#include<stdio.h>
int main()
{
    int a[100],i,j,smallest,n,temp,position;
    scanf("%d",&n);
    printf("enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        if(a[i]>a[j])
        {
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
        }
    }
    scanf("%d",&position);
    printf("%d",a[position-1]);
return 0;
}

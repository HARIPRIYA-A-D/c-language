#include<stdio.h>
int main()
{
    int a[100],n,i;
    int b[100]={},c[100]={};
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]%2!=0)
        {
           b[i]=a[i]; 
        }
        else
        {
            c[i]=a[i];
        }
    }
    for(i=0;i<n;i++)
    {
        if(b[i]!=0)
        printf("%d",b[i]);
    }
    for(i=0;i<n;i++)
    {
        if(c[i]!=0)
        printf("%d",c[i]);
    }
    return 0;
}

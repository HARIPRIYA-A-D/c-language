#include<stdio.h>
int main()
{
    int n,a[100],i,temp,count=0;
    printf("enter the size:");
    scanf("%d",&n);
    printf("enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    temp=a[0];
    printf("%d\n",temp);
    for(i=0;i<n;i++)
    {
        if(a[i]>temp)
        {
            count++;
        }
    }
    printf("%d",count);
    
    return 0;
    
}

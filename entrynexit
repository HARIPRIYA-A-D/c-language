#include<stdio.h>
int main()
{
    int e[100],l[100];
    int i,t;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        scanf("%d\n",&e[i]);
    }
    for(i=0;i<t;i++)
    {
        scanf("%d",&l[i]);
    }
    int sum=0,max=0;
    for(i=0;i<t;i++)
    {
        sum=sum+e[i]-l[i];
    
    if(sum>max)
    {
        max=sum;
    }
    
    }
    printf("%d",max);
    
    return 0;
}
